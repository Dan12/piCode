#ram status
free -m
#shutdown
sudo shutdown -h now
#reboot
sudo shutdown -r now
#storage
df -h
#memory usage
top
#rename directory/move directory/rename file/move file
mv [from] [to]
#check directory size
du -h -c [dir]
#make file
cat > [filename]
#c compile
gcc [filename.c] [options] [-lwiringPi]
#c++ compile
g++ [filename]
